    "pg": "7.11.0",
    "sequelize": "5.8.6",
    "socket-io": "1.0.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "dotenv": "8.0.0",
    "faker": "4.1.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1"
  }
}
